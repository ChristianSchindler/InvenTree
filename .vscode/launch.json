  "version": "0.2.0",
  "configurations": [
    {
      "name": "InvenTree Server",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/InvenTree/manage.py",
      "args": ["runserver"],
      "django": true,
      "justMyCode": true
    },
    {
      "name": "Python: Django - 3rd party",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/InvenTree/manage.py",
      "args": ["runserver"],
      "django": true,
      "justMyCode": false
    },
    {
      "name": "Python: Django - test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/InvenTree/manage.py",
      "args": ["test", "plugin.base.label.test_label_mixin.LabelMixinTests.test_printing_process"],
      "django": true,
      "justMyCode": false
    }
  ]
}
